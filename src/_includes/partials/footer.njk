<footer>
  <div class="{{ colors.footerCustom.barBg or colors.footerDefault.barBg }}" style="background-color:#905937">
    <div class="max-w-screen-xl px-4 py-12 mx-auto sm:px-6 md:flex md:items-center md:justify-between lg:px-8">
      <div class="flex justify-center md:order-2 {{ colors.socialIconsCustom or colors.socialIconsDefault }}">
        {% if structure.g_showFooterSocialIcons %}

          {# Display Social icons from _data/social.json  #}
          {% for site in social %}
            {% if site.url %}
              <a aria-label="Link to {{ site.platform }}" alt="Link to {{ site.platform }}" class="hover:{{ colors.socialIconsHoverCustom or colors.socialIconsHoverDefault }}" href="{{ site.url | url }}">
                {% svg site.platform, "mx-2 h-6 w-6 fill-current", site.platform, "footer" %}
              </a>
            {% endif %}
          {% endfor %}

        {% endif %}
      </div>
      <div class="mt-8 md:mt-0 md:order-1">
        <p class="text-center text-base leading-6 {{ colors.footerCustom.text or colors.footerDefault.text }}" style="color:#E9E990">
          &copy; {{ build.timestamp | dateToFormat('yyyy') }}, {{ "Keith Taylor" }} <br />
          ｟<a href="/" style="background-color:#E9E990">Home</a>｠ | ｟<a href="/about-keith-taylor/" style="background-color:#E9E990">About</a>｠ | ｟<a href="/keith-taylor-purpose/" style="background-color:#E9E990">Purpose</a>｠ | ｟<a href="/contact-keith-taylor/" style="background-color:#E9E990">Contact</a>｠ | ｟<a href="/keith-taylor-privacy/" style="background-color:#E9E990">Privacy</a>｠ | ｟<a href="/keith-taylor-terms-and-conditions/" style="background-color:#E9E990">T&Cs</a>｠ | ｟<a href="/keith-taylor-news/" style="background-color:#E9E990">Notifications</a>｠ | ｟<a href="/help-keith-taylor/" style="background-color:#E9E990">Help</a>｠ | ｟<a href="https://cse.google.com/cse?cx=e7158312c712ab2c3" style="background-color:#E9E990">Search</a>｠
        </p>
      </div>
    </div>
  </div>
  <script type="text/javascript">
    (function(c,l,a,r,i,t,y){
        c[a]=c[a]||function(){(c[a].q=c[a].q||[]).push(arguments)};
        t=l.createElement(r);t.async=1;t.src="https://www.clarity.ms/tag/"+i;
        y=l.getElementsByTagName(r)[0];y.parentNode.insertBefore(t,y);
