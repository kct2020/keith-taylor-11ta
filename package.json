{
  "name": "keith-taylor-11ta",
  "version": "1.0.0",
  "description": "Source for keith-taylor.pages.dev",
  "author": "Keith Taylor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kct2020/keith-taylor-11ta/issues"
  },
  "homepage": "https://github.com/kct2020/keith-taylor-11ta#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/kct2020/keith-taylor-11ta.git"
  },
  "keywords": [
    "11ty",
    "TailwindCSS",
    "TailwindUI",
    "Alpine.js",
    "webpack",
    "PostCSS",
    "CSSnano"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "dev:webpack": "webpack  --mode development",
    "dev:eleventy": "eleventy --serve",
    "dev:postcss": "postcss src/assets/css/tailwind.css -o dist/assets/tailwind.css --watch",
    "dev": "npm-run-all clean --parallel dev:* --print-label",
    "build:webpack": "webpack  --mode production",
    "build:eleventy": "ELEVENTY_ENV=production eleventy",
    "build:postcss": "NODE_ENV=production postcss src/assets/css/tailwind.css -o dist/assets/tailwind.css",
    "build": "run-s clean build:* --print-label"
  },
  "devDependencies": {
    "@11ty/eleventy": "1.0.0",
    "@11ty/eleventy-navigation": "0.3.2",
    "@11ty/eleventy-plugin-rss": "1.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "4.0.0",
    "@babel/core": "7.17.8",
    "@babel/preset-env": "7.16.11",
    "alpinejs": "3.9.5",
    "autoprefixer": "10.4.4",
    "axios": "0.26.1",
    "babel-loader": "8.2.4",
    "clean-css": "5.2.4",
    "cssnano": "5.1.5",
    "dotenv": "16.0.0",
    "html-minifier": "4.0.0",
    "luxon": "2.3.1",
    "markdown-it": "12.3.2",
    "markdown-it-emoji": "2.0.0",
    "npm-run-all": "4.1.5",
    "postcss-cli": "9.1.0",
    "postcss-import": "14.1.0",
    "rimraf": "3.0.2",
    "slugify": "1.6.5",
    "svg-sprite": "1.3.7",
    "tailwindcss": "3.0.23",
    "terser-webpack-plugin": "5.3.1",
    "webpack": "5.70.0",
    "webpack-cli": "4.9.2"
  }
}
